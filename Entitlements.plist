<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Камера и микрофон -->
    <key>com.apple.security.device.camera</key>
    <true/>
    <key>com.apple.security.device.audio-input</key>
    <true/>

    <!-- Сеть -->
    <key>com.apple.security.network.client</key>
    <true/>
    <key>com.apple.security.network.server</key>
    <true/>

    <!-- Уведомления -->
    <key>com.apple.security.personal-information.notifications</key>
    <true/>

    <!-- Фото -->
    <key>com.apple.security.assets.movies</key>
    <true/>
    <key>com.apple.security.assets.music</key>
    <true/>
    <key>com.apple.security.assets.pictures</key>
    <true/>

    <!-- Локальная сеть -->
    <key>com.apple.security.network.local</key>
    <true/>
</dict>
</plist>
